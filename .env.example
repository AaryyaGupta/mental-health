# Backend Environment Variables
# Copy to .env and fill in secure values before starting the server.

PORT=5000
NODE_ENV=development

# OpenAI API
OPENAI_API_KEY=sk-...

# Supabase (public anon key ONLY used on frontend; service role key ONLY on backend)
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=eyJhbGciOi...
SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOi... # keep secret (server only)

# Google OAuth (server side exchange)
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Public base URL (optional; used for OAuth redirect override)
PUBLIC_URL=http://localhost:5000
OPENAI_API_KEY=your_openai_key_here
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_SERVICE_ROLE=service_role_key_never_expose_frontend
SUPABASE_ANON_KEY=anon_public_key_for_frontend
PUBLIC_URL=http://localhost:5000
ALLOWED_ORIGINS=http://localhost:5000